ns1:bundleid {
    <http://openprovenance.org/var#quote> a prov:Entity ;
        prov:value "var:value"^^xsd:string ;
        prov:wasAttributedTo <http://openprovenance.org/var#author> .

    <http://openprovenance.org/var#author> a prov:Entity,
            "prov:Person"^^xsd:string ;
        ns2:name "var:name"^^xsd:string .
}

